<body class="">

    <!-- Add your content of header -->

    <main class="content-wrapper">
        <header class="white-text-container section-container">
            <div class="text-center">
                <h1>Manage Your Post </h1>



            </div>
            <!-- Search form -->

        </header>



        <!-- Add your site or app content here -->
        @foreach (var p in Model)
        {
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">

                        <div class="card">
                            <div class="card-block">
                                <h2>@p.job</h2>
                                <div class="row">

                                    <div class="col-md-8">

                                        <p>@p.name</p>
                                        <p>@p.phoneNumber</p>
                                        <p>@p.email</p>
                                        <p>@p.address</p>
                                        <p>@p.details</p>
                                        <div class="container" class="text-center">
                                            <div class="row">
                                                <div class="col-md-12 col-sm-12" class="text-center" >
                                                    <a href="/hireme/edit/@p.id" class="smoothScroll btn btn-danger" Text-align: center>      Edit      </a>
                                                    <a href="/hireme/delete/@p.id" class="smoothScroll btn btn-danger" Text-align: center>      Delete      </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }



        


